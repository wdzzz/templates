{
  "log": {
    "version": "1.2",
    "creator": {
      "name": "Chrome",
      "version": "140.0.0.0"
    },
    "entries": [
      // 1. 获取登录页（用于提取 formhash 和 loginhash）
      {
        "request": {
          "method": "GET",
          "url": "https://www.jkju.cc/member.php?mod=logging&action=login",
          "headers": [
            { "name": "User-Agent", "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/140.0.0.0 Safari/537.36 Edg/140.0.0.0" },
            { "name": "Referer", "value": "https://www.jkju.cc/" }
          ]
        },
        "response": {
          "status": 200,
          "headers": [],
          "content": { "size": 0 }
        }
      },
      // 2. 提交登录表单
      {
        "request": {
          "method": "POST",
          "url": "https://www.jkju.cc/member.php?mod=logging&action=login&loginsubmit=yes&inajax=1&loginhash={{loginhash}}",
          "headers": [
            { "name": "User-Agent", "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/140.0.0.0 Safari/537.36 Edg/140.0.0.0" },
            { "name": "Origin", "value": "https://www.jkju.cc" },
            { "name": "Referer", "value": "https://www.jkju.cc/member.php?mod=logging&action=login" },
            { "name": "Content-Type", "value": "application/x-www-form-urlencoded" }
          ],
          "postData": {
            "mimeType": "application/x-www-form-urlencoded",
            "params": [
              { "name": "formhash", "value": "{{formhash}}" },
              { "name": "referer", "value": "https://www.jkju.cc/" },
              { "name": "username", "value": "{{username}}" },
              { "name": "password", "value": "{{password}}" },
              { "name": "loginfield", "value": "{{loginfield}}" }, // username 或 email
              { "name": "cookietime", "value": "2592000" }
            ]
          }
        },
        "response": { "status": 200 }
      },
      // 3. 获取签到页（提取签到 formhash）
      {
        "request": {
          "method": "GET",
          "url": "https://www.jkju.cc/plugin.php?id=zqlj_sign",
          "headers": [
            { "name": "User-Agent", "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/140.0.0.0 Safari/537.36 Edg/140.0.0.0" }
          ]
        },
        "response": { "status": 200 }
      },
      // 4. 提交签到请求
      {
        "request": {
          "method": "GET",
          "url": "https://www.jkju.cc/plugin.php?id=zqlj_sign&sign={{sign_hash}}",
          "headers": [
            { "name": "User-Agent", "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/140.0.0.0 Safari/537.36 Edg/140.0.0.0" },
            { "name": "Referer", "value": "https://www.jkju.cc/plugin.php?id=zqlj_sign" }
          ]
        },
        "response": { "status": 200 }
      }
    ]
  }
}
